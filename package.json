    "@vue/test-utils": "^2.4.1",
    "eslint": "^8.51.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-vue": "^9.17.0",
    "happy-dom": "^12.9.1",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3",
    "vite": "^4.5.0",
    "vitepress": "^1.0.0-rc.22",
    "vitest": "^0.34.6",
